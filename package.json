	"private": true,
	"scripts": {
		"build": "next build",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"check": "npm run build && tsc",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"dev": "next dev",
		"lint": "next lint",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"start": "next start",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "tsx scripts/init-local-db.ts",
		"db:migrate:prod": "wrangler d1 migrations apply group-name-db --remote",
		"db:studio": "drizzle-kit studio",
		"db:push:local": "drizzle-kit push --config=drizzle.config.ts",
		"db:push:prod": "drizzle-kit push --config=drizzle.config.ts --config=productionConfig",
		"db:seed": "tsx scripts/seed.ts"
	}
